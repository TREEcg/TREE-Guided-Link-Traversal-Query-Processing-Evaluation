            "parameterRangeElements": [
              "xsd:string",
              {
                "@type": "ParameterRangeUndefined"
              }
            ]
          },
          "comment": "Where to find the configuration templates."
        },
        {
          "@id": "css:dist/pods/generate/TemplatedPodGenerator.jsonld#TemplatedPodGenerator_storeFactory",
          "range": "css:dist/pods/generate/ComponentsJsFactory.jsonld#ComponentsJsFactory",
          "comment": "Factory used for Components.js instantiation."
        },
        {
          "@id": "css:dist/pods/generate/TemplatedPodGenerator.jsonld#TemplatedPodGenerator_variableHandler",
          "range": "css:dist/pods/generate/variables/VariableHandler.jsonld#VariableHandler",
          "comment": "Handler used for setting variable values."
        },
        {
