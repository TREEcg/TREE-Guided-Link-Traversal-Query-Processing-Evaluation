        {
          "@id": "css:dist/pods/generate/TemplatedPodGenerator.jsonld#TemplatedPodGenerator_baseUrl",
          "range": "xsd:string"
        },
        {
          "@id": "css:dist/pods/generate/TemplatedPodGenerator.jsonld#TemplatedPodGenerator_configTemplatePath",
          "range": {
